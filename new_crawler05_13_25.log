事件详情，目前警方正在调查中。\u003c/P\u003e\u003c!--PARAGRAPH_4--\u003e\u003c!--NO_AD_4--\u003e\u003cP\u003e来源：红星新闻综合\u003c/P\u003e","version":"v1"},"originAttribute":{"IMG_0":{"bigOrigUrl":"https://inews.gtimg.com/news_bt/O1npW6eAemiYuL_HWNowEUHqdxZ5Ejdu-sk2LORt54QMQAA/0","compressUrl":"https://inews.gtimg.com/news_bt/O1npW6eAemiYuL_HWNowEUHqdxZ5Ejdu-sk2LORt54QMQAA/641","desc":"","fullPic":"1","height":1395,"imgurl0":"https://inews.gtimg.com/news_bt/O1npW6eAemiYuL_HWNowEUHqdxZ5Ejdu-sk2LORt54QMQAA/0","imgurl1000":"https://inews.gtimg.com/news_bt/O1npW6eAemiYuL_HWNowEUHqdxZ5Ejdu-sk2LORt54QMQAA/1000","islong":0,"origUrl":"https://inews.gtimg.com/news_bt/O1npW6eAemiYuL_HWNowEUHqdxZ5Ejdu-sk2LORt54QMQAA/641","size":259,"style":"visibility: visible; width: 677px","thumb":"https://inews.gtimg.com/news_bt/O1npW6eAemiYuL_HWNowEUHqdxZ5Ejdu-sk2LORt54QMQAA_181x181s/0","url":"https://inews.gtimg.com/news_bt/O1npW6eAemiYuL_HWNowEUHqdxZ5Ejdu-sk2LORt54QMQAA/641","width":641},"IMG_1":{"bigOrigUrl":"https://inews.gtimg.com/news_bt/OoAdAusJ30CLoemdsnt6Am65qdE2hQGEimlFp2dbmG390AA/0","compressUrl":"https://inews.gtimg.com/news_bt/OoAdAusJ30CLoemdsnt6Am65qdE2hQGEimlFp2dbmG390AA/641","desc":"","fullPic":"1","height":546,"imgurl0":"https://inews.gtimg.com/news_bt/OoAdAusJ30CLoemdsnt6Am65qdE2hQGEimlFp2dbmG390AA/0","imgurl1000":"https://inews.gtimg.com/news_bt/OoAdAusJ30CLoemdsnt6Am65qdE2hQGEimlFp2dbmG390AA/1000","islong":0,"origUrl":"https://inews.gtimg.com/news_bt/OoAdAusJ30CLoemdsnt6Am65qdE2hQGEimlFp2dbmG390AA/641","size":623,"style":"-webkit-tap-highlight-color: transparent; border-style: solid; border-width: 1px; outline: 0px; vertical-align: middle; visibility: visible; width: 676.984px","thumb":"https://inews.gtimg.com/news_bt/OoAdAusJ30CLoemdsnt6Am65qdE2hQGEimlFp2dbmG390AA_181x181s/0","url":"https://inews.gtimg.com/news_bt/OoAdAusJ30CLoemdsnt6Am65qdE2hQGEimlFp2dbmG390AA/641","width":641},"IMG_2":{"bigOrigUrl":"https://inews.gtimg.com/news_bt/O-b9m6oxZTTY5kf2e7zsQ9bvZvuzeBDNltrdBVeBglFiMAA/0","comp