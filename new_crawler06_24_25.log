cle_from":"","article_id":"20250620A04HA4","atype":"0","audio":[{"duration":200,"gender":"male","path":"/ol/wx_male_2_20250620A04HA400.mp3","speaker_id":"wx_male","subtitles":"","type":2}],"audio_info":{"audio_url":"","content_duration":200,"content_path":"/ol/wx_male_2_20250620A04HA400.mp3","duration":0,"path":""},"available_info":{"reason":"","status":1},"candidate_scale_covers":{"discover_ratio":0,"discover_url":""},"category_id":"229","category_unified":{"cate1_en_name":"society","cate1_id":"20242855","cate1_name":"社会","cate2_id":"3067291","cate2_name":"正能量","note":{"cate1_check":0,"cate1_org":"社会","cate1_status":"pipe_free"}},"chlid":"26123","chlname":"政知见","cid":"8421671649","cms_orig_info":{"cmsid":"20250620A04HA400","desc":"张军桥不幸牺牲，年仅38岁","ori_url":"https://mp.weixin.qq.com/s?__biz=MzA5NTY5MzUxMQ==\u0026mid=2651177981\u0026idx=1\u0026sn=43d585a84a8a783ad8c2f10bb0b48eef\u0026scene=21#wechat_redirect","trust_level":1,"type":"","url":"https://view.inews.qq.com/a/20250620A04HA400"},"cms_plugin_img":{"big_img":"https://inews.gtimg.com/news_ls/Okz_r6_OgCqZZ5aLapE1etBY6ohFkmDUapoAjJL5s7TlkAA_752320/0","small_img":"https://inews.gtimg.com/news_ls/OSnq5RvzGMsnNQb8Za7KJPGAHBuwvw11AHU-XKuOJdk9UAA/0"},"cms_text_safe_level":{"safe_level":1},"cmsid":"20250620A04HA400","cnt_top":null,"co_creation_info":{"participators":null},"content":[{"desc":"\u003cstrong\u003e编辑 | 董鑫\u003c/strong\u003e","type":"cnt_article"},{"desc":"据澎湃新闻6月20日消息，6月15日，中国援助坦桑尼亚医疗队队长、麻醉科专家张军桥医生在达累斯萨拉姆Kigamboni海域英勇施救一名落水民众时，不幸溺水牺牲，年仅38岁。","type":"cnt_article"},{"desc":"张医生在现场发现一位坦桑尼亚女性落水后，毫不犹豫跃入海中施救，并将自己的救生衣让给落水者，成功将其托举上岸，但自己却因体力不支被水流卷走，不幸牺牲。","type":"cnt_article"},{"desc":"张军桥是中国派驻坦桑尼亚穆希比利国家医院（Muhimbili National Hospital）的高级麻醉专家，拥有医学、外科、内科学和麻醉学多重学位，并持有覆盖全球100多个国家的国际职业执照。","type":"cnt_article"},{"desc":"2020年2月13日，他曾作为山东省第十批援助湖北医疗队的一员，前往黄冈武穴市妇幼保健院援助近两个月的时间。援鄂结束回潍坊后，张军桥又积极投身于推广心肺复苏的公益活动中，利用下班后或者周末时间在潍坊市人民广场“出摊”，身着绿色隔离衣为过往的市民公益讲解心肺复苏术，被大家称为“地摊医生”。","type":"cnt_article"},{"desc":"在张军桥的带动下，来自潍坊医学院附属医院、潍坊市人民医院、潍坊市第二人民医院的十几位医护人员也纷纷加入到他的“地推心肺复苏”队伍中来。","type":"cnt_article"},{"desc":"2024年1月，张军桥刚来坦桑尼亚时，就拟定了初步培训工作规划，但当地医疗设备的匮乏程度，还是超出了他的预期。调研后，他根据实际调整工作重点，不仅引入国际先进的医疗技术，更着力培养当地医疗队伍。","type":"cnt_article"},{"desc":"为解决当地医院“困难气道”（医学术语，是指在临床实践中，由于患者气道解剖结构异常、病理改变或外界因素干扰，面罩通气、气管插管、喉镜检查等常规操作难以顺利进行的一种情况）的难题，张军桥通过所在的山东第二医科大学和山东第二医科大学附属医院，向当地麻醉科捐赠了价值10多万元人民币的可视喉镜，并逐步在当地医院建立“困难气道”插管临床路径。在穆希比利国家医院，张军桥还专门安排了固定时间，对当地医生进行培训。","type":"cnt_article"},{"desc":"8岁的坦桑尼亚小学生路易斯·阿里·卡托次被发现患有巨大腹壁疝，高昂的医疗费用让这个贫困的家庭一度陷入绝望。第27批援坦桑尼亚中国医疗队成员们帮助患儿前往穆希比利国家医院诊治，并承担了全部医疗费用，才让他重获新生。","type":"cnt_article"},{"desc":"出院前，这对母子特地找到张军桥，为孩子取个中国名字，希望以此铭记中国医疗队的恩情。“就叫他‘齐鲁’吧，那代表我们祖国的山东省，也是我们这组医疗队员共同的故乡，愿两国人民心手相连、情谊永恒。”张军桥说。","type":"cnt_article"},{"desc":"","img_url":"https://inews.gtimg.com/news_bt/Oz-El_KAjNmwR4zBrSdInGe4Eus8XPe-hmFKayFmk4KlUAA/641","itype":2,"type":"img_url"},{"desc":"","img_url":"https://inews.gtimg.com/news_bt/OMWDs9dDEaEb82D-YICm5gNUdzqbclTrk7nZhVHB-wEIYAA/641","itype":2,"type":"img_url"},{"desc":"","img_url":"https://inews.gtimg.com/news_bt/OmBD7SsivTv7h15-iJmsyEwJ8tH6wgfAJNlXnSXJkWHNIAA/641","itype":2,"type":"img_url"},{"desc":"","img_url":"https://inews.gtimg.com/news_bt/Ob7izc3XO2FT0v5sfS-jM-7f8kS9li6XX1l62HnfBX1kQAA/641","itype":2,"type":"img_url"}],"content_action":{"close_ai_comment":0,"close_all_ad":0,"close_show_push":0,"emoticon":"","event_line_show_flag":0,"expression_switch":0,"favorite":0,"is_all_pic_expand":0,"is_open_advertisement":1,"is_open_comment_ad":1,"is_open_pics_ad":1,"is_open_related_news_ad":1,"is_open_text_ad":1,"is_open_wx_bot_ad":1,"is_sole":0,"open_related_news_ad":0,"political_option":0,"read_count_not_display":0},"content_picked_imgs":null,"controversial_topics":{"msg":"tag:不是争议性话题","res":"0"},"copyright_status":"","cp_info":{"backflow_gears":1,"backflow_plan":1,"category_id":"1","category_name":"新闻","certification_info":"北京青年报政知见官方账号","certification_level":1,"certification_type":3,"cp_text_dis_status":3,"cp_video_dis_status":3,"cr_op_entertain":-1,"director":"haoyanglen